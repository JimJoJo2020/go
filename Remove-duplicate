#include<bits/stdc++.h>
using namespace std;

int RemDup(int arr[], int n) {
    int res = 1;
    for(int i = 1; i < n ;i++) {
        if(arr[i] != arr[res-1]){
        arr[res] = arr[i];
        res++;
        }
    }
    return res;
}

int main() {
    int arr[] = {5, 9, 9,24,24,24,24, 24, 118, 215};
    int n = sizeof(arr) / sizeof(arr[0]);

    
    n = RemDup(arr,n);
    for (int i = 0; i < n; i++)
        cout << arr[i] << " ";
 
    
    return 0;
}
